{
  "name": "Rice Home",
  "version": "1",
  "manifest_version": 2,
  "description": "This Chrome extension provides everything for a Rice student",
  "incognito": "spanning",
//  "homepage_url": "http://hack.rice.edu",
  "icons": {
    "16": "icons/rice.png",
    "48": "icons/rice.png",
    "128": "icons/rice.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}